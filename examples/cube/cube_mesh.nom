bank tp1
    set     n       8       3       10      1
    set     ro      1     0     3.14     0.1
    set     ratio   0    -0.5    0.5     0.1
    set     h       1.5     0.1     1.5     0.1
endbank

tunnel tun1 ({expr $tp1_n} {expr cos($tp1_ro)} {expr $tp1_ratio} {expr $tp1_h}) endtunnel

instance t tun1 endinstance